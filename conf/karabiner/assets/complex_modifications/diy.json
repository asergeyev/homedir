{
  "title": "DIY",
  "rules": [
    {
      "description": "Command+= to run iTerm",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "equal_sign",
            "modifiers": {
              "mandatory": ["left_command"]
            }
          },
          "to": [
            {
              "shell_command": "automator '/Users/asergeyev/Documents/Open Terminal.workflow'"
            }
          ]
        }
      ]
    },
    {
      "description": "Command+F1..F6 to Command+1..6",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "f1",
            "modifiers": {
              "mandatory": ["left_command"]
            }
          },
          "to": [
            {
              "key_code": "1",
              "modifiers": ["left_command"]
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "f2",
            "modifiers": {
              "mandatory": ["left_command"]
            }
          },
          "to": [
            {
              "key_code": "2",
              "modifiers": ["left_command"]
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "f3",
            "modifiers": {
              "mandatory": ["left_command"]
            }
          },
          "to": [
            {
              "key_code": "3",
              "modifiers": ["left_command"]
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "f4",
            "modifiers": {
              "mandatory": ["left_command"]
            }
          },
          "to": [
            {
              "key_code": "4",
              "modifiers": ["left_command"]
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "f5",
            "modifiers": {
              "mandatory": ["left_command"]
            }
          },
          "to": [
            {
              "key_code": "5",
              "modifiers": ["left_command"]
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "f6",
            "modifiers": {
              "mandatory": ["left_command"]
            }
          },
          "to": [
            {
              "key_code": "6",
              "modifiers": ["left_command"]
            }
          ]
        }
      ]
    },
    {
      "description": "Home and End to Control + a and Control + e",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "home",
            "modifiers": {
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "a",
              "modifiers": ["left_control"]
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "end",
            "modifiers": {
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "e",
              "modifiers": ["left_control"]
            }
          ]
        }
      ]
    }
  ]
}
